{
  "architecture": "DenseNet",
  "dataset_name": "CIFAR100",
  "batch_size": 256,

  "optimizer": {
    "name": "Momentum",
    "momentum": 0.9,
    "use_nesterov": false
  },

  "learning_rate": 0.1,
  "num_epochs": 300,

  "lr_annealing": {
    "name": "epoch_decay_boundaries",
    "epoch_decay_boundaries": [150, 225],
    "lr_decay": 0.1
  },

  "pre_processing":
  {
    "train":
    {
      "random_flip_left_right": true
    },
    "test":
    {
    }
  },

  "growth_rate": 32,
  "reduction": 0.5,
  "build_method": "sandbox",
  "depth": 169,
  "dropout_rate": 0.25,

  "weight_decay": 1e-4,
  "test_every": 1,
  "seed": 1232,
  "ranks": [96, 96]
}
