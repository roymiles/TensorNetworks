{
  "architecture": "DenseNet",
  "dataset_name": "CIFAR100",
  "batch_size": 128,

  "optimizer": {
    "name": "Momentum",
    "momentum": 0.9,
    "use_nesterov": false
  },

  "learning_rate": 0.1,
  "num_epochs": 300,

  "lr_annealing": {
    "name": "cosine_decay"
  },

  "pre_processing":
  {
    "train":
    {
      "pad": true,
      "random_crop": {"width": 32, "height": 32},
      "random_flip_left_right": true
    },
    "test":
    {
    }
  },

  "growth_rate": 12,
  "reduction": 0.5,
  "build_method": "sandbox",

  "depth": "notusedm8",
  "stages": [12, 12, 12, 12],

  "dropout_rate": 0.25,

  "weight_decay": 1e-4,
  "test_every": 1,
  "seed": 1232,
  "ranks": [96, 96]
}
