{
  "architecture": "MobileNetV1",
  "dataset_name": "cifar100",

  "num_epochs": 200,
  "img_height": 32,
  "img_width": 32,
  "num_channels": 3,
  "num_classes": 100,
  "batch_size": 256,

  "optimizer": "Momentum",
  "momentum": 0.9,
  "use_nesterov": true,
  "learning_rate": 0.1,
  "learning_rate_decay": 0.2,
  "num_epochs_decay": 60,

  "test_every": 1,
  "seed": 42
}
