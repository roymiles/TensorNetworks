{
  "architecture": "MobileNetV1",
  "dataset_name": "CIFAR100",
  "batch_size": 128,
  "weight_decay": 5e-4,

  "optimizer": {
    "name": "Momentum",
    "momentum": 0.9,
    "use_nesterov": false
  },

  "learning_rate": 2e-2,
  "num_epochs": 300,

  "lr_annealing": {
    "name": "epoch_decay_boundaries",
    "epoch_decay_boundaries": [5, 50, 100, 150, 200],
    "lr_decay": 0.5
  },

  "pre_processing":
  {
    "train":
    {
      "random_flip_left_right": true
    },
    "test":
    {
    }
  },

  "method": "custom-bottleneck",
  "ranks": [64, 64],
  "partitions": [0.2, 1.0]
}
