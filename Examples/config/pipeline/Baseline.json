{
  "architecture": "MobileNetV1",
  "dataset_name": "CIFAR10",
  "batch_size": 128,

  "optimizer": "Momentum",
  "momentum": 0.9,
  "use_nesterov": false,

  "learning_rate": 0.1,
  "learning_rate_decay": 0.1,
  "epoch_decay_boundaries": [150, 250],
  "num_epochs": 1,

  "weight_decay": 5e-4,
  "switch_list": [1.0],

  "test_every": 1,
  "seed": 1232,

  "method": "standard",
  "ranks": [128, 128],
  "partitions": [0.2, 0.5]
}
